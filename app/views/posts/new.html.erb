<h1>New Post</h1>
<%= form_with :model => @posts do |form| %>
  <p>
    <%= form.label "image"%>
    <%= form.text_field "image" %>
  </p>
  <p>
    <%= form.label "body" %>
    <%= form.text_field "body" %>
    <%= form.label "author" %>
    <%= form.text_field "author" %>
  </p>
  <%= form.submit %>
<% end %>